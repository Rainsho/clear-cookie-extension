{
  "name": "Clear Cookie",
  "description": "Clear Cookie by Sites",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["cookies", "tabs", "http://*/*", "https://*/*"],
  "icons": { "16": "cookie.png", "48": "cookie.png", "128": "cookie.png" },
  "browser_action": {
    "default_popup": "main.html",
    "default_icon": "cookie.png"
  }
}
